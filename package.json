{
  "name": "site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prettier": "prettier --write '*.js' 'src/**/*.js'",
    "docusaurus": "docusaurus",
    "generate-graphql": "mkdir -p docs/graphql && docusaurus graphql-to-doc --force && sed --in-place -e '/Generated on/d' docs/graphql/generated.md && sed --in-place -e 's/Schema Documentation/GraphQL Schema/g' docs/graphql/generated.md",
    "generate-grpc": "mkdir -p protodocs && docusaurus generate-proto-docs && find . -type f -name '*.mdx'",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids"
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-beta.15",
    "@docusaurus/preset-classic": "2.0.0-beta.15",
    "@easyops-cn/docusaurus-search-local": "^0.21.4",
    "@edno/docusaurus2-graphql-doc-generator": "1.8.2",
    "@graphql-tools/url-loader": "7.5.3",
    "@mdx-js/react": "1.6.22",
    "@saucelabs/theme-github-codeblock": "^0.1.1",
    "@svgr/webpack": "5.5.0",
    "clsx": "1.1.1",
    "docusaurus-protobuffet": "0.3.1",
    "file-loader": "6.2.0",
    "graphql": "^16.0.1",
    "netlify-cli": "6.14.0",
    "prettier": "2.4.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "redocusaurus": "0.6.0",
    "url-loader": "4.1.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
